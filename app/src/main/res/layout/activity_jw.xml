<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bcsn"
    tools:context=".jw">
    <TextView
        android:id="@+id/textView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#9EFFFFFF"
        android:gravity="center"
        android:text="PENGANTIN JAWA"
        android:textColor="@color/black"
        android:textSize="12pt"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrjw"
        android:layout_width="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_height="125dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/jw1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrjw1"
        android:layout_width="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_height="125dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/jw2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrjw3" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrjw2"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:src="@drawable/jw3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrjw4" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrjw3"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/jw4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrjw" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrjw4"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginTop="8dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginEnd="32dp"
        android:src="@drawable/jw5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrjw5" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrjw5"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:src="@drawable/jw6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/btn_kmbljw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="@color/teal_700"
        android:gravity="center"
        android:text="KEMBALI"
        android:textSize="8pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrjw2" />

</androidx.constraintlayout.widget.ConstraintLayout>