<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bcsn"
    tools:context=".bl">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#9EFFFFFF"
        android:gravity="center"
        android:text="PENGANTIN BALI"
        android:textColor="@color/black"
        android:textSize="12pt"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrbl"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/bl1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrbl1"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/bl2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrbl3" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrbl2"
        android:layout_width="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_height="125dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:src="@drawable/bl3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrbl3"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/bl5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrbl" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrbl4"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginTop="16dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginEnd="32dp"
        android:src="@drawable/bl6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrbl5" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_glrbl5"
        android:layout_width="125dp"
        android:layout_height="125dp"
        app:civ_border_color="#81FFFFFF"
        app:civ_border_width="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:src="@drawable/bl7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrbl2" />

    <Button
        android:id="@+id/btn_kmblbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"

        android:backgroundTint="@color/teal_700"
        android:gravity="center"
        android:text="KEMBALI"
        android:textSize="8pt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_glrbl1" />

</androidx.constraintlayout.widget.ConstraintLayout>